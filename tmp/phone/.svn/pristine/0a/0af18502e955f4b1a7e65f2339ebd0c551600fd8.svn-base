.disbute-modal
{
  .fist-panel {
    .panel-body {
      padding: 0;
      .scope-ul {
        padding: 0;
        height: 445px;
        overflow: auto;
        li {
          list-style: none;
          cursor: pointer;
          padding: 10px;
          &hover{
            background: #8a8dd2;
            color: #fff;
          }
          &focus {
            background: #8a8dd2;
            color: #fff;
          }
        }
        .slted-ul {
          background: #8a8dd2;
          color: #fff;
        }
      }
    }
  }
  .scond-panel{
    .panel-body {
      height: 445px;
      overflow: auto;
      .mbr-box {
        border-bottom: 1px solid #ddd;
        .table {
          margin-bottom: 0px;
          tr{
            th {
              border: none;
            }
            td {
              border: none;
            }
          }
        }
      }
    }
  }
  .thid-panel
  {
    .panel-body {
      padding: 0;
      .sel-box{
        .tip {
          color: #b0b3b9;
          text-indent: 12px;
          height: 360px;
          width: 100%;
          overflow: auto;
          border: none;
          resize: none;
          &focus{
            border: 1px solid #a5c7fe;
          }
          &hover {
            border: 1px solid #a5c7fe;
          }
        }
        .sel-p {
          height: 60px;
          line-height: 60px;
          background: #f6f7f8;
          margin-top: 20px;
          .sel-btn {
            background: #49d3c6;
            display: inline-block;
            color: #fff;
          }
        }
      }
    }
  }
}

.disbute-modal .thid-panel .panel-body .sel-box
.disbute-modal .thid-panel .panel-body .sel-box .sel-p
.add-group-modal textarea {
  resize: none;
}
