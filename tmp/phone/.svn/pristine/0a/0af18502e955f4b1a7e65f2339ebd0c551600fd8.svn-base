/*
 * 修正bootstrap
 */
.form-inline{
  .btn-group-sm {
    display: inline-block;
  }
}
.btn-sm {
  width: 120px;
  margin-right: 10px;
}
/*基础样式*/
/*
 * base.less
 * 定义 html 与 body 标签的样式， 包括字体和字体大小等……
 */
html,
body
{
  width: 100%;
  height: 100%;
  min-width: 1024px;
  font-size: 14px;
  background: #3b4a62;
  font-family: Arial, 'Microsoft YaHei', 'SimSun';
}
/*表格里面的删除功能链接的样式*/
.data-del
{
  margin-left: 10px;
}
/*删除提示图表*/
.msg-prompt
{
  font-size: 40px;
  vertical-align: middle;
  margin-right: 20px;
}
/*页面错误提示信息*/
.error-msg
{
  position: fixed;
  top: 81px;
  width: 500px;
  left: 50%;
  margin-left: -250px;
  background-color: #f5eab0;
  border: none;
}
/*无返回数据提醒*/
.no-data
{
  width: 100%;
  height: 100px;
  line-height: 100px;
  background: #fff;
  font-size: 18px;
  color: #b0b3b9;
}
.no-data i
{
  font-size: 32px;
  vertical-align: middle;
  margin-left: 10px;
}
/*通用的最小宽度*/
.min-width50
{
  min-width: 50px;
}
.min-width100
{
  min-width: 100px;
}
.min-width150
{
  min-width: 150px;
}
.grail-basic-styles
{
 /* #grail-content
  {
    div
    {
      height: 100%;
    }
  }
  #grail-aside {
    div
    {
      height: 100%;
    }
  }*/
  .tree-search
  {
    width: 95px;
    height: 25px;
  }
  .tree-btn
  {
    padding: 0;
    width: 25px;
    height: 25px;
    display: inline-block;
    margin-bottom: 0;
    font-size: 14px;
    font-weight: normal;
    line-height: 1.42857143;
    text-align: center;
    white-space: nowrap;
    vertical-align: middle;
    -ms-touch-action: manipulation;
    touch-action: manipulation;
    cursor: pointer;
    -webkit-user-select: none;
    -moz-user-select: none;
    -ms-user-select: none;
    user-select: none;
    background-image: none;
    border: 1px solid transparent;
    border-radius: 4px;
  }
  .tree-search-btn
  {
    background: url(../assets/images/search.png) no-repeat center #44afd1;
  }
  .tree-unfold-btn
  {
    background: url(../assets/images/spread.png) no-repeat center #49d3c6;
  }
  .tree-fold-btn
  {
    background: url(../assets/images/combine.png) no-repeat center #49d3c6;
  }
  .tree-container {
    padding-right: 15px;
    position: relative;
    .tree-area {
      height: 100%;
      width: 100%;
      overflow: auto;
      background: #fff;
    }
    .fold-btn {
      width: 15px;
      position: absolute;
      right: 0;
      top: 0;
      bottom: 0;
      border: none;
      background: #fff;
      padding: 0;
      outline: none;
      color: #8d8d8d;
    }
    .fold-btn {
      &hover {
        background: #e9e9e9;
      }
      &active {
        box-shadow: 0 0 5px #878787 inset;
      }
    }
  }
  .navbar-dropmenu-container
  {
    display: none;
    position: absolute;
    top: 80px;
    left: 0;
    background: #b0b3b9;
    z-index: 9999;
    .list-group{
      border-radius: 0;
      border: none;
      background: #666eb9;
      color: #fff;
      margin: 0;
      cursor: pointer;
      padding: 0;
      a{
        display: block;
        width: 100%;
        height: 100%;
        padding: 10px 15px;
        color: #fff;
      }
    }
    .list-group-item
    {
      border-radius: 0;
      border: none;
      background: #666eb9;
      color: #fff;
      margin: 0;
      cursor: pointer;
      padding: 0;
      a
      {
        display: block;
        width: 100%;
        height: 100%;
        padding: 10px 15px;
        color: #fff;
      }
      &hover
      {
        background: #727AC6;
        box-shadow: 2px 2px 5px #333;
        .second-level-menu{
          display:none;
        }
      }
    }
    .first-level-menu
    {
      display: none;
      width: 180px;
      position: relative;
      .second-level-menu
      {
        display: none;
        position: absolute;
        width: 180px;
        top: 0;
        left: 180px;
      }
    }
    .navdrop-mask {
      position: fixed;
      top: 0;
      left: 0;
      right: 0;
      bottom: 0;
      z-index: -1;
    }
  }
}

.first-level-menu
{
  overflow: auto;
  height: 100%;
  li
  {
    line-height: 48px;
    border-bottom: #333;
    a {
      padding-left: 5px;
      padding-right: 5px;
    }
    span {
      padding-left:10px;
      a
      {
        color: #fff;
        font-size: 16px;
      }
    }
    .second-level-menu
    {
      background: #314057;
      color: #fff;
      padding: 0;
      li {
        list-style: none;
        line-height: 40px;
        padding: 0;
        color: #fff;
        padding-left: 40px;
        cursor: pointer;
        a {
          color: #fff;
        }
      }
    }
  }
  li
  {
    a {
      color: #fff;
      font-size: 16px;
    }
  }
}

