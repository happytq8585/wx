.quickstart-login {
  width: 100%;
  height: 100%;
}
.quickstart-login .login {
  width: 100%;
  height: 100%;
  z-index: 1;
  position: absolute;
  background: url("../assets/images/login/login-bg.png") no-repeat center;
}
.quickstart-login .login .log-con .logo2 {
  margin-top: 11px;
  height: 453px;
  width: 431px;
  background: url("../assets/images/login/loginbox-bg.png") no-repeat;
  display: block;
  float: left;
  position: relative;
}
.quickstart-login .login .log-con .logo2 .login-text {
  height: 150px;
  width: 71px;
  position: absolute;
  left: 50%;
  margin-left: -75px;
  top: 50%;
  margin-top: -50px;
}
.quickstart-login {
  height: 453px;
  width: 856px;
  position: fixed;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  margin: auto;
  padding-left: 10px;
  padding-right: 10px;
  background: url("../assets/images/login/login-box.png") no-repeat center;
}
.quickstart-login .log-con .row {
  height: 511px;
  width: 386px;
  display: block;
  float: left;
  padding-left: 10px;
  padding-right: 10px;
}
.quickstart-login .log-con .row .con-body {
  height: 427px;
  width: 356px;
  margin: 50px 25px;
}
.quickstart-login .log-con .row .con-body .log-int {
  padding-left: 8px;
  padding-right: 8px;
}
.quickstart-login .log-con .row .con-body .log-int input {
  height: 36px;
  /*width: 100%;*/
  border: 1px solid #ddd;
  text-indent: 10px;
  background: #fff;
  margin-bottom: 20px;
  color: #333;
  line-height: 38px;
  padding-right: 10px;
}
.quickstart-login .log-con .row .con-body .log-int div {
  display: inline;
}
.quickstart-login .log-con .row .con-body .log-int label {
  color: #333;
  font-size: 14px;
  line-height: 24px;
}
.quickstart-login .log-con .row .con-body .log-int select {
  height: 36px;
  /*  width: 100%;*/
  border: 1px solid #ddd;
  text-indent: 10px;
  background: #fff;
  margin-bottom: 20px;
  color: #333;
  line-height: 36px;
}
.quickstart-login .log-con .row .con-body .log-int .ck-rmUser {
  border: 1px solid #dddddd;
  text-indent: 10px;
  background: #fff;
  color: #333;
  height: 13px;
  padding-left: 10px;
  padding-right: 10px;
}
.quickstart-login .log-con .row .con-body .log-int .ck-rmUser .logChose {
  line-height: 13px;
}
.quickstart-login .log-con .tit {
  margin: 15px;
  text-align: center;
  border-bottom: 1px solid #ddd;
  font-weight: bold;
  color: #3797d9;
}
.quickstart-login .log-con .tit span {
  margin: auto;
  font-size: 24px;
  line-height: 56px;
  letter-spacing: 15px;
}
.txt-right {
  text-align: right;
}
.txt-align {
  text-align: right;
}
